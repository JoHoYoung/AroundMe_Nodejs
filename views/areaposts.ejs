                        </div>

                    </div>
                </div>

                <script>
                        var x=document.getElementById("map<%=areagroup%>");
                        x.style.fill="#3b8eff";
                        var temp=<%= areagroup%>;
                        var whatarea = document.getElementsByClassName("whatarea");
                        var str;
                        switch (temp) {
                            case 0:
                                str = "도심(종로, 중구, 용산)";
                                break;
                            case 1:
                                str = "서서울(은평, 마포, 서대문)";
                                break;
                            case 2:
                                str = "서남(강서, 양천, 영등포, 구로)";
                                break;
                            case 3:
                                str = "남서울(동작, 관악, 금천)";
                                break;
                            case 4:
                                str = "강남(서초, 강남)";
                                break;
                            case 5:
                                str = "동남(강동, 송파)";
                                break;
                            case 6:
                                str = "동대문(중랑, 성동, 광진)";
                                break;
                            case 7:
                                str = "강북(도봉, 강북, 성북, 노원)";
                                break;

                        }
                    for(var i=0;i<whatarea.length;i++)
                        whatarea[i].innerHTML=str;

                    
                    
                    var cssfile = document.styleSheets[0].rules[0];
                    document.addEventListener('mousemove', evt => {
                        let x = evt.clientX;
                        let y = evt.clientY;
                        cssfile.style.setProperty('--mouse-x', (x - 65) + "px");
                        cssfile.style.setProperty('--mouse-y', (y - 85) + "px");

                    });
                    function btnclick() {
                        document.getElementById("mbox").classList.toggle("show");

                    }
                    function setAction() {
                        var ac = document.getElementById("searchstring").value;
                        document.getElementById("search").action = "/areaposts/search/" + <%= areagroup%> + "/" + ac + "/1";
                        return true;

                    }
                </script>
            </div>
        <% include ../views/rfooter%>
